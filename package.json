{
  "name": "dapr-durabletask",
  "version": "1.0.0",
  "description": "A Durable Task Javascript SDK compatible with Dapr Workflow and its underlying Durable Task engine",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "test:unit": "jest test/unit --runInBand --detectOpenHandles",
    "test:e2e": "jest test/e2e --runInBand --detectOpenHandles",
    "start": "ts-node --swc ./src/index.ts",
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node --swc' ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XavierGeerinck/durabletask-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XavierGeerinck/durabletask-js/issues"
  },
  "homepage": "https://github.com/XavierGeerinck/durabletask-js#readme",
  "devDependencies": {
    "@swc/core": "^1.3.55",
    "@swc/helpers": "^0.5.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "regenerator-runtime": "^0.13.11",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "google-protobuf": "^3.21.2"
  }
}
